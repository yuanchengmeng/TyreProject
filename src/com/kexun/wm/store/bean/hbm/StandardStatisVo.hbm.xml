<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kexun.wm.store.bean">

	<class name="StandardStatisVo" table="standard_statis">
		<!-- 映射主键 -->
		<comment></comment>
		<composite-id>
            <key-property name="storeType" type="java.lang.String" column="storeType"></key-property>
            <key-property name="classes" type="java.lang.String" column="classes"></key-property>
            <key-property name="person" type="java.lang.String" column="person"></key-property>
            <key-property name="productID" type="integer" column="productID"></key-property>
        </composite-id> 
		<!-- 映射基本属性 --> 
		<property name="amount" type="integer">
			<column name="amount">
				<comment></comment>
			</column>
		</property>
		 
 	</class>
</hibernate-mapping>










