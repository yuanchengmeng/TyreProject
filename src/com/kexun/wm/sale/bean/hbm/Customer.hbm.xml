<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kexun.wm.sale.bean">

	<class name="Customer" table="t_Organization">
		<!-- 映射主键 -->
		<id name="FItemID" column="FItemID" type="java.lang.Integer">
			<generator class="identity" />
		</id>
 		<!-- 映射基本属性 -->
		<property name="FMobilePhone" type="java.lang.String">
			<column name="FMobilePhone">
				<comment>手机号码</comment>
			</column>
		</property>
		<property name="FName" type="java.lang.String">
			<column name="FName">
				<comment>客户名称</comment>
			</column>
		</property>
		<property name="FNumber" type="java.lang.String">
			<column name="FNumber">
				<comment>客户代码</comment>
			</column>
		</property>
		<property name="FPhone" type="java.lang.String">
			<column name="FPhone">
				<comment>电话号码</comment>
			</column>
		</property>
		<property name="FProvince" type="java.lang.String">
			<column name="FProvince">
				<comment>省份</comment>
			</column>
		</property>
		<property name="FRegion" type="java.lang.Integer">
			<column name="FRegion">
				<comment>国别地区</comment>
			</column>
		</property>
		<property name="FSaleID" type="java.lang.Integer">
			<column name="FSaleID">
				<comment></comment>
			</column>
		</property>
		<property name="FShortName" type="java.lang.String">
			<column name="FShortName">
				<comment>客户简称</comment>
			</column>
		</property>
		<property name="FShortNumber" type="java.lang.String">
			<column name="FShortNumber">
				<comment>客户简码</comment>
			</column>
		</property>
		<property name="FStatus" type="java.lang.Integer">
			<column name="FStatus">
				<comment>状态</comment>
			</column>
		</property>
		<property name="FAddrEn" type="java.lang.String">
			<column name="FAddrEn">
				<comment>英文地址</comment>
			</column>
		</property>
		<property name="FAddress" type="java.lang.String">
			<column name="FAddress">
				<comment>地址</comment>
			</column>
		</property>
		<property name="FBrNo" type="java.lang.String">
			<column name="FBrNo">
				<comment>公司代码</comment>
			</column>
		</property>
		<property name="FCity" type="java.lang.String">
			<column name="FCity">
				<comment>城市</comment>
			</column>
		</property>
		<property name="FClassTypeID" type="java.lang.Integer">
			<column name="FClassTypeID" not-null="false">
				<comment>单据编码</comment>
			</column>
		</property>
		<property name="FContact" type="java.lang.String">
			<column name="FContact">
				<comment>联系人</comment>
			</column>
		</property>
		<property name="FCountry" type="java.lang.String">
			<column name="FCountry">
				<comment>国家</comment>
			</column>
		</property>
		<property name="FEmail" type="java.lang.String">
			<column name="FEmail">
				<comment>邮件地址</comment>
			</column>
		</property>
		<property name="Femployee" type="java.lang.Integer">
			<column name="Femployee">
				<comment>专营业务员</comment>
			</column>
		</property>
		<property name="FErpClsID" type="java.lang.Integer">
			<column name="FErpClsID">
				<comment>产品类别</comment>
			</column>
		</property>
		<property name="FfavorPolicy" type="java.lang.String">
			<column name="FfavorPolicy">
				<comment>优惠政策</comment>
			</column>
		</property>
		<property name="FFax" type="java.lang.String">
			<column name="FFax">
				<comment>传真号</comment>
			</column>
		</property>
		<property name="FFlat" type="java.lang.Integer">
			<column name="FFlat">
				<comment>交易客户</comment>
			</column>
		</property>
		<property name="FHomePage" type="java.lang.String">
			<column name="FHomePage">
				<comment>公司主页</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>










