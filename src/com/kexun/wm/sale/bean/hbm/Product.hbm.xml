<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kexun.wm.sale.bean">

	<class name="Product" table="t_ICItem">
		<!-- 映射主键 -->
		<id name="FItemID" column="FItemID" type="java.lang.Integer">
			<generator class="identity" />
		</id>
 		<!-- 映射基本属性 -->
		<property name="FLength" type="float">
			<column name="FLength">
				<comment>长度</comment>
			</column>
		</property>
		<property name="FMaund" type="java.lang.Integer">
			<column name="FMaund">
				<comment>单位</comment>
			</column>
		</property>
		<property name="FModel" type="java.lang.String">
			<column name="FModel">
				<comment>规格型号</comment>
			</column>
		</property>
		<property name="FName" type="java.lang.String">
			<column name="FName">
				<comment>产品名称</comment>
			</column>
		</property>
		<property name="FNumber" type="java.lang.String">
			<column name="FNumber">
				<comment>产品代码</comment>
			</column>
		</property>
		<property name="FHelpCode" type="java.lang.String">
			<column name="FHelpCode">
				<comment>助记符</comment>
			</column>
		</property>
		<property name="FOrderPrice" type="float">
			<column name="FOrderPrice">
				<comment>订货单价</comment>
			</column>
		</property>
		<property name="FPlanPrice" type="float">
			<column name="FPlanPrice">
				<comment>计划单价</comment>
			</column>
		</property>
		<property name="FSaleAcctID" type="java.lang.Integer">
			<column name="FSaleAcctID">
				<comment>销售科目</comment>
			</column>
		</property>
		<property name="FSalePrice" type="float">
			<column name="FSalePrice">
				<comment>销售单价</comment>
			</column>
		</property>
		<property name="FSaleUnitID" type="java.lang.Integer">
			<column name="FSaleUnitID">
				<comment>销售计量单位</comment>
			</column>
		</property> 
		<property name="FShortNumber" type="java.lang.String">
			<column name="FShortNumber">
				<comment>产品简码</comment>
			</column>
		</property>
		<property name="FStoreUnitID" type="java.lang.Integer">
			<column name="FStoreUnitID">
				<comment>库存计量单位</comment>
			</column>
		</property>
		<property name="FTypeID" type="java.lang.Integer">
			<column name="FTypeID">
				<comment>产品分类</comment>
			</column>
		</property>
		<property name="FUnitGroupID" type="java.lang.Integer">
			<column name="FUnitGroupID">
				<comment>单位组内码</comment>
			</column>
		</property>
		<property name="FUnitID" type="java.lang.Integer">
			<column name="FUnitID">
				<comment>单位内码</comment>
			</column>
		</property>
		<property name="FBrNo" type="java.lang.String">
			<column name="FBrNo">
				<comment>公司代码</comment>
			</column>
		</property>
		<property name="FClass" type="java.lang.Integer">
			<column name="FClass">
				<comment>产品类别</comment>
			</column>
		</property>
		<property name="FDepartment" type="java.lang.Integer">
			<column name="FDepartment">
				<comment>部门</comment>
			</column>
		</property>
		<property name="FErpClsID" type="java.lang.Integer">
			<column name="FErpClsID">
				<comment>产品属性</comment>
			</column>
		</property> 
		<property name="FFullName" type="java.lang.String">
			<column name="FFullName">
				<comment>全名</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>










