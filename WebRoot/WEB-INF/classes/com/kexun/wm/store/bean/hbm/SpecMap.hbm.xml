<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kexun.wm.store.bean">

	<class name="SpecMap" table="specmap">
		<!-- 映射主键 -->
		<comment></comment>
		<id name="standardName" column="standardname" type="java.lang.String">
			<generator class="identity" />
		</id>

		<!-- 映射基本属性 -->
		<property name="brandName" type="java.lang.String">
			<column name="brandname" length="255">
				<comment></comment>
			</column>
		</property>
		<property name="speed" type="java.lang.String">
			<column name="speed" length="255">
				<comment></comment>
			</column>
		</property>
	</class>
</hibernate-mapping>










